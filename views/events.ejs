<%- include("partials/header") %>
<section class="container-fluid sectionMargin">
    <div class="row">
        <article class="col-md-3">
            <h1 class="display-5">Reserve Entry Today!</h1>
            <p class="lead">Join us for our upcoming event inspired by the ocean front venue. A whimsical rendition of frozen treats.</p>
        </article>
        <div class="col-md-6">
            <img src="/images/events1.svg" class="img-fluid" alt="Responsive image">
        </div>
        <article class="col-md-3">
            <form>
                <div class="form-group">
                    <label for="name">Name</label>
                    <input type="text" class="form-control" id="name" placeholder="Amelia Lincoln">
                </div>
                <div class="form-group">
                  <label for="email">Email Address</label>
                  <input type="email" class="form-control" id="email" placeholder="name@example.com">
                </div>
                <div class="form-group">
                    <label for="party-number">Group Size</label>
                    <select class="form-control" id="party-number">
                      <option>How many in your party?</option>
                      <option>1</option>
                      <option>2</option>
                      <option>3</option>
                      <option>4</option>
                    </select>
                  </div>
                <div class="form-group">
                  <label for="exampleFormControlSelect1">Choose A Day</label>
                  <select class="form-control" id="exampleFormControlSelect1">
                    <option>Available Dates--</option>
                    <option>Jan-22nd</option>
                    <option>Jan-23rd</option>
                    <option>Jan-24th</option>
                  </select>
                </div>
                <button id="submit" class="btn btn-primary" type="submit" >Submit</button>
              </form>
        </article>
    </div>
</section>

